{
  "name": "@studioalex/oui",
  "version": "0.1.0",
  "description": "A lightweight Vue 3 component library",
  "author": "Alexander Vu <alexander.vu.tuyet@gmail.com>",
  "license": "UNLICENSED",
  "files": [
    "dist/oui.es.js",
    "dist/style.css",
    "dist/oui.umd.js"
  ],
  "types": "src/oui.d.ts",
  "main": "./dist/oui.umd.js",
  "module": "./dist/oui.es.js",
  "style": "./dist/style.css",
  "exports": {
    "./package.json": "./package.json",
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/oui.esm.ts",
      "require": "./src/oui.ts"
    }
  },
  "unpkg": "./dist/oui.umd.js",
  "jsdelivr": "./dist/oui.umd.js",
  "scripts": {
    "lib:build": "vite build",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "dev": "vitepress dev docs",
    "build": "vite build && vitepress build docs",
    "lint": "eslint --ext .js,.vue src",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "analyze": "vite build --config analyze.config.js",
    "publish": "npm publish --access public",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf packages/oui/docs/.vitepress/dist && rm -rf stats.html"
  },
  "keywords": [
    "vue",
    "ui",
    "oui"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.5",
    "postcss": "^8.4.5",
    "postcss-nested": "^5.0.6",
    "rollup": "^2.61.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "vitepress": "^0.20.5",
    "vue-tsc": "^0.29.8"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "tabbable": "^5.2.1",
    "tippy.js": "^6.3.7",
    "vue": "^3.2.26"
  }
}
